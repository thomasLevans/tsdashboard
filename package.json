{
  "name": "tsdashboard",
  "version": "1.0.0",
  "description": "A dashboard for viewing research publications in the dimension of translational science",
  "main": "src/app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "istanbul cover node_modules/mocha/bin/_mocha",
    "app:start": "node ./dist/build.min.js",
    "wp": "cd ./config",
    "wp:dev": "npm run wp | webpack-dev-server --progress --colors",
    "wp:deploy": "npm run wp | webpack -p --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jobeid/tsdashboard.git"
  },
  "keywords": [
    "D3",
    "Translational",
    "Science",
    "MUSC",
    "BMIC",
    "Bioinformatics"
  ],
  "author": "Thomas Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jobeid/tsdashboard/issues"
  },
  "homepage": "https://github.com/jobeid/tsdashboard#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "mr-doc": "^3.2.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
